(define-module (riscv instruction rv_m)
  #:use-module (riscv build-base)
  #:use-module (riscv range-base)
  #:export (mul mulh mulhsu mulhu div divu rem remu)
)

(define mul (make-instruction rd rs1 rs2 (range-fixed 31 25 1) (range-fixed 14 12 0) (range-fixed 6 2 12) (range-fixed 1 0 3)))
(define mulh (make-instruction rd rs1 rs2 (range-fixed 31 25 1) (range-fixed 14 12 1) (range-fixed 6 2 12) (range-fixed 1 0 3)))
(define mulhsu (make-instruction rd rs1 rs2 (range-fixed 31 25 1) (range-fixed 14 12 2) (range-fixed 6 2 12) (range-fixed 1 0 3)))
(define mulhu (make-instruction rd rs1 rs2 (range-fixed 31 25 1) (range-fixed 14 12 3) (range-fixed 6 2 12) (range-fixed 1 0 3)))
(define div (make-instruction rd rs1 rs2 (range-fixed 31 25 1) (range-fixed 14 12 4) (range-fixed 6 2 12) (range-fixed 1 0 3)))
(define divu (make-instruction rd rs1 rs2 (range-fixed 31 25 1) (range-fixed 14 12 5) (range-fixed 6 2 12) (range-fixed 1 0 3)))
(define rem (make-instruction rd rs1 rs2 (range-fixed 31 25 1) (range-fixed 14 12 6) (range-fixed 6 2 12) (range-fixed 1 0 3)))
(define remu (make-instruction rd rs1 rs2 (range-fixed 31 25 1) (range-fixed 14 12 7) (range-fixed 6 2 12) (range-fixed 1 0 3)))
